{
	"compilerOptions": {
		"target": "esnext",
		"module": "preserve",
		"stripInternal": true,
		"moduleResolution": "bundler",
		"noEmit": true,
		"checkJs": true,
		"declaration": true,
		"allowJs": true,
		"alwaysStrict": true,
		"resolvePackageJsonExports": true,
		"resolvePackageJsonImports": true,
		"resolveJsonModule": true
	}
}
